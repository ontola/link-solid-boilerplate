{
  "name": "link-mash-app-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate to set up link apps which can be run stand-alone and loaded as plugins",
  "main": "dist/package/package.js",
  "export": {
    ".": "dist/package/package.js",
    "app": "dist/app/app.js",
    "pane": "dist/pane/pane.js"
  },
  "author": "Fletcher91 <thom@ontola.io>",
  "license": "GPL-3.0-or-later",
  "private": false,
  "solid": {
    "applicationURL": {
      "development": "http://localhost:8000/",
      "production": "_enter the url your application will be run from_"
    },
    "name": "_my app name_"
  },
  "scripts": {
    "rebuild": "yarn run clean && yarn run build",
    "build": "webpack",
    "clean": "rm -rf dist/app/* && rm -rf dist/package/* && rm -rf dist/pane/*",
    "deploy": "gh-pages -d ./dist/app"
  },
  "dependencies": {
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.6.0",
    "@ontola/mash": "^0.5.3",
    "@ontola/memoized-hash-factory": "^1.1.1",
    "@ontologies/core": "=1.5.2-strict-20",
    "@ontologies/dcterms": "^1.0.0",
    "@ontologies/foaf": "^1.0.0",
    "@ontologies/owl": "^1.0.0",
    "@ontologies/rdf": "^1.0.0",
    "@ontologies/rdfs": "^1.0.0",
    "@ontologies/schema": "^1.0.0",
    "@ontologies/xsd": "^1.0.0",
    "classnames": "^2.2.6",
    "clsx": "^1.0.4",
    "director": "^1.2.8",
    "http-status-codes": "^1.4.0",
    "link-lib": "=1.0.0-rc4-datafactory-15",
    "link-redux": "=1.0.0-rc4-datafactory-9",
    "n-quads-parser": "=2.0.3-ontologies",
    "rdflib": "1.0.7-0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-is": "16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-transform-react-jsx": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@ontola/link-devtools": "^1.1.1",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "gh-pages": "^2.1.1",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "import-to-array": "^1.0.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "solid-auth-client": "^2.4.1",
    "style-loader": "^1.0.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "globals": {
      "FRONTEND_ROUTE": true
    }
  }
